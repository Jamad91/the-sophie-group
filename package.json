{
  "name": "the-sophie-group",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "validate": "check-node-version --node '>= 6.7.0'",
    "setup": "./bin/setup",
    "prep": "npm run validate && npm run setup",
    "postinstall": "npm run prep",
    "build": "webpack",
    "build-watch": "npm run build -- -w",
    "build-dev": "cross-env NODE_ENV=development npm run build-watch",
    "build-branch": "bin/build-branch.sh",
    "start": "node server/start.js",
    "start-watch": "nodemon server/start.js --watch server --watch db",
    "start-dev": "cross-env NODE_ENV=development npm run start-watch",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others --prefix \"[{name}]\" --names \"BUILD,SERVE\" -c \"bgBlue.bold.black,bgGreen.bold.black\" \"npm run build-dev\" \"npm run start-dev\"",
    "test": "mocha --compilers js:babel-register app/**/*.test.js app/**/*.test.jsx db/**/*.test.js server/**/*.test.js",
    "test-watch": "npm run test -- --watch",
    "seed": "node db/seed.js",
    "deploy-heroku": "bin/deploy-heroku.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamad91/the-sophie-group.git"
  },
  "keywords": [
    "react",
    "redux",
    "skeleton"
  ],
  "author": "Jimmy DiColandrea",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/queerviolet/the-sophie-group/issues"
  },
  "homepage": "https://github.com/jamad91/the-sophie-group/#readme",
  "dependencies": {
    "axios": "^0.15.2",
    "babel-preset-stage-2": "^6.18.0",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "chai-enzyme": "^0.5.2",
    "chalk": "^1.1.3",
    "check-node-version": "^1.1.2",
    "concurrently": "^3.1.0",
    "cookie-session": "^2.0.0-alpha.1",
    "enzyme": "^2.5.1",
    "express": "^4.14.0",
    "finalhandler": "^1.0.0",
    "google-maps-react": "^1.0.19",
    "jquery": "^3.2.1",
    "nuka-carousel": "^2.0.4",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.10",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "pretty-error": "^2.0.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-image-gallery": "^0.7.15",
    "react-image-slider": "^0.1.0",
    "react-promise": "^1.1.2",
    "react-redux": "^4.4.5",
    "react-responsive-carousel": "^3.1.5",
    "react-router": "^3.0.0",
    "react-slick": "^0.14.7",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "sass": "^0.5.0",
    "sequelize": "^3.24.6",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "slick-carousel": "^1.6.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0",
    "volleyball": "^1.4.1",
    "webpack": "^2.2.1",
    "webpack-livereload-plugin": "^0.10.0"
  },
  "description": "To get it up and running: 1) git clone 2) npm install 3) npm run dev",
  "bin": {
    "the-sophie-group": "mkapplink.js"
  }
}
